{
  "name": "pancake-subgraph",
  "description": "PancakeSwap Subgraph",
  "version": "2.0.0",
  "repository": "git@github.com:Moonster-BSC/pancake-subgraph.git",
  "author": "PancakeSwap",
  "license": "MIT",
  "scripts": {
    "codegen:exchange": "graph codegen subgraphs/exchange.yaml",
    "build:exchange": "graph build subgraphs/exchange.yaml",
    "deploy:exchange": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ moonster-bsc/pancake-pair subgraphs/exchange.yaml",
    "format:check": "prettier --check */**/*.{js,ts}",
    "format:write": "prettier --write */**/*.{js,ts}",
    "lint": "eslint */**/*.{js,ts}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format:write"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.3",
    "@commitlint/config-conventional": "^12.1.3",
    "@graphprotocol/graph-cli": "^0.20.0",
    "@graphprotocol/graph-ts": "^0.20.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^4.3.8",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  }
}
